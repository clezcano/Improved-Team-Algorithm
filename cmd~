#!/bin/sh


  g++ -o /usr/share/pvm3/bin/LINUXI386/master -I/usr/share/pvm3/include -I/usr/share/pvm3/src mastercat.cc -L/usr/share/pvm3/lib/LINUXI386 -lpvm3

  g++ -o /usr/share/pvm3/bin/LINUXI386/slave -I/usr/share/pvm3/include -I/usr/share/pvm3/src slavecat.cc -L/usr/share/pvm3/lib/LINUXI386 -lpvm3
